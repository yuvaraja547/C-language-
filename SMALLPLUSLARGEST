#include <stdio.h>

int main()
{
  int a,b,c,d,max,min;
  scanf("%d %d %d %d",&a,&b,&c,&d);
  int big= a>b&&a>c&&a>d ? a:b>c&&b>d? b:c>d?c :d;
  int small= a<b&&a<c&&a<d ?a:b<c&&b<d?b:c<d?:d;
  printf("%d\n",big);
  printf("%d\n",small);
  int result=big+small;
  printf("%d \n",result);

    return 0;
}
