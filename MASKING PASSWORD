
#include <stdio.h>
#include<string.h>

int main()
{
  char ch;
  char password[20];
  int i=0;
  printf("enter any password:");
  
  while((ch=getch())!=13)
  {
      password[i]=ch;
      i=i+1;
      printf(" *");
  }
  password[i]="\0";
  printf("entered password is %s,password");
  
  

    return 0;
}
