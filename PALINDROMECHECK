#include <stdio.h>
#include<string.h>
int main()
{
  char a[50];
  printf("enetr the string array:");
  scanf("%s",a);
int  l=strlen(a);
  int i,flag=0;
  for(i=0;i<l/2;i++)
  {
      if(a[i]!=a[l-i-1])
      flag=1;
      break;
      
  }
  if (flag)
  printf(" not palindrome");
  else
  printf("palindrome");
return 0;
}
    
